{
  "name": "@chenberti/street-piano-sdk",
  "version": "1.0.0",
  "description": "This SDK wraps mqtt.js and EMQX features to provide a robust, production-ready MQTT consumer for backend services.",
  "keywords": [
    "mqtt",
    "client",
    "sdk",
    "street-piano"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henberti/street-piano-sdk.git"
  },
  "license": "MIT",
  "author": "Hen Berti",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "mqtt": "^5.14.1",
    "uuid": "^13.0.0"
  }
}
